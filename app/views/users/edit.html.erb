<%= form_with model: @user, local: true do |form| %>
    <div class="form-group">
      <%= form.label :profile_picture, "Upload Profile Picture" %>
      <%= form.file_field :profile_picture %>
    </div>
  
    <div class="form-group">
      <%= form.submit "Update Profile", class: "btn btn-primary" %>
    </div>
  <% end %>
  